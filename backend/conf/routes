GET /              controllers.HomeController.index()
GET /tables        controllers.HomeController.tables()
GET /login          controllers.UserController.login()
GET /posts          controllers.PostController.getAll()
GET /image/:id   controllers.PostController.getImage(id)

POST /           controllers.PostController.addPost()
POST /register   controllers.UserController.register()
POST    /comment/:id    controllers.CommentController.addComment(id)